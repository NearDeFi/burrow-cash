{
  "name": "burrow-cash",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "parcel build src/index.html --public-url ./ --no-cache",
    "clean": "rm -rf public && rm -rf dist && rm -rf .parcel-cache",
    "analyze": "BUNDLE_BUDDY=true parcel build src/index.html --reporter @parcel/reporter-bundle-buddy",
    "start": "parcel src/index.html --open",
    "prod": "CONTRACT_NAME=contract.main.burrow.near NEAR_ENV=mainnet DEFAULT_NETWORK=mainnet parcel src/index.html",
    "deploy:pages": "gh-pages -d dist/",
    "deploy": "npm run build && npm run deploy:pages",
    "prettier": "prettier ./src --write",
    "lint": "prettier --check \"./src/**/*.{ts,tsx}\" && eslint \"./src/**/*.{ts,tsx}\" && yarn type-check",
    "type-check": "tsc --pretty --noEmit",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/preset-env": "~7.16.4",
    "@babel/preset-react": "~7.16.0",
    "@parcel/config-default": "^2.2.1",
    "@parcel/reporter-bundle-buddy": "^2.2.1",
    "@parcel/transformer-svg-react": "^2.2.1",
    "@parcel/transformer-typescript-tsc": "^2.2.1",
    "@types/luxon": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "~27.3.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "jest": "~27.3.1",
    "jest-environment-node": "~27.3.1",
    "near-cli": "~2.2.0",
    "parcel": "2.2.1",
    "prettier": "^2.4.1",
    "react-test-renderer": "~17.0.2"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/lab": "^5.0.0-alpha.59",
    "@mui/material": "^5.1.1",
    "@mui/styles": "^5.1.1",
    "@mui/system": "^5.1.1",
    "@mui/utils": "^5.1.1",
    "@near-wallet-selector/core": "v3.0.0",
    "@near-wallet-selector/near-wallet": "v3.0.0",
    "@near-wallet-selector/sender": "v3.0.0",
    "@react-icons/all-files": "^4.1.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@sentry/react": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "@svgr/parcel-plugin-svgr": "^5.5.0",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.181",
    "@types/node": "^16.11.9",
    "@types/ramda": "^0.27.59",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-virtualized": "^9.21.15",
    "@types/styled-components": "^5.1.15",
    "bn.js": "^5.2.0",
    "borsh": "^0.6.0",
    "decimal.js": "^10.3.1",
    "dotenv": "^10.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "framer-motion": "^6.3.0",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "near-api-js": "~0.44.2",
    "millify": "^4.0.0",
    "pluralize": "^8.0.0",
    "posthog-js": "^1.20.2",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-use": "^17.3.2",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "regenerator-runtime": "~0.13.9",
    "timeago-react": "^3.0.4",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/jest.init.js"
    ],
    "testEnvironment": "near-cli/test_environment",
    "testPathIgnorePatterns": [
      "<rootDir>/contract/",
      "<rootDir>/node_modules/"
    ]
  },
  "browserModern": "public/index.html",
  "targets": {
    "browserModern": {
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
    }
  },
  "engines": {
    "node": ">=14"
  }
}
